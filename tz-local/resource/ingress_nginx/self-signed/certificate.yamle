apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: self-signed-cert-
spec:
  secretName: self-signed-cert
  duration: 8760h
  renewBefore: 360h
  issuerRef:
    name: selfsigned-issuer
    kind: ClusterIssuer
    group: cert-manager.io
  commonName: .drillquiz.com
  privateKey:
    algorithm: ECDSA
    size: 256
  dnsNames:
    - .drillquiz.com
    - .default.topzone-k8s.drillquiz.com
